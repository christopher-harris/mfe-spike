{
  "name": "mfe-spike",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:shell": "ng serve shell --port 5000",
    "start:dashboard": "ng serve dashboard --port 3000",
    "start:all": "concurrently \"npm run start:shell\" \"npm run start:dashboard\" ",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "test:ci": "ng test --watch=false --code-coverage --source-map true",
    "build:ci": "npm run clean && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^12.5.3",
    "@angular/animations": "~12.1.0",
    "@angular/cdk": "^12.2.8",
    "@angular/common": "~12.1.0",
    "@angular/compiler": "~12.1.0",
    "@angular/core": "~12.1.0",
    "@angular/forms": "~12.1.0",
    "@angular/material": "^12.2.8",
    "@angular/platform-browser": "~12.1.0",
    "@angular/platform-browser-dynamic": "~12.1.0",
    "@angular/router": "~12.1.0",
    "concurrently": "^6.3.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.4",
    "@angular/cli": "~12.1.4",
    "@angular/compiler-cli": "~12.1.0",
    "@types/node": "^12.11.1",
    "ng-packagr": "^12.1.0",
    "typescript": "~4.3.2"
  }
}
